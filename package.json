  "devDependencies": {
    "@nestjs/testing": "^11.1.8",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "nestjs",
    "nest",
    "gcp",
    "google-cloud",
    "pubsub",
    "messaging",
    "message-queue",
    "publish-subscribe",
    "event-driven",
    "events",
    "module",
    "publisher",
    "subscriber",
    "listener",
    "emulator"
  ]
